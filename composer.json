{
    "name": "maxfactor/support",
    "description": "Support and helpers for Laravel PHP based projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "marco.mark@dewsign.co.uk",
            "homepage": "https://dewsign.co.uk"
        }
    ],
    "require": {
        "laravel/framework": "^5",
        "symfony/intl": "^4.0",
        "dannyvankooten/vat.php": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Maxfactor\\Support\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3"
    },
    "extra":{
        "laravel": {
            "providers": [
                "Maxfactor\\Support\\MaxfactorServiceProvider"
            ],
            "aliases": {
                "Maxfactor": "Maxfactor\\Support\\Facades\\Maxfactor",
                "Format": "Maxfactor\\Support\\Facades\\Format"
            }
        }
    }
}
